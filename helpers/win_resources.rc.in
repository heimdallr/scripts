        BLOCK "000904b0"
        BEGIN
            VALUE "CompanyName", "${ORGANIZATION_NAME}"
            VALUE "FileDescription", "${PRODUCT_NAME_FILE_DESCRIPTION}"
            VALUE "FileVersion", "${PRODUCT_VERSION_MAJOR}.${PRODUCT_VERSION_MINOR}.0"
            VALUE "InternalName", "${ORGANIZATION_NAME} ${PRODUCT_NAME}"
            VALUE "OriginalFilename", "${TARGET_OUTPUT_NAME}.exe"
            VALUE "ProductName", "${ORGANIZATION_NAME} ${PRODUCT_NAME_VERSIONED}"
            VALUE "ProductVersion", "${PRODUCT_VERSION_MAJOR}.${PRODUCT_VERSION_MINOR}.0"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x9, 1200
    END
END
